# ADSP FastRPC
service vendor.adsprpcd /odm/bin/adsprpcd
   class main
   user media
   group media
   disabled

service vendor.audiopd /odm/bin/adsprpcd audiopd
   class main
   user media
   group media
   disabled

on property:ro.board.platform=sdm660
    enable vendor.audiopd

on property:vendor.qcom.adspup=1
    enable vendor.adsprpcd
